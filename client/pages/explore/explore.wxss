page {
  background-color: rgb(221, 221, 221);
}

.explore {
	margin: 0 40rpx;
	padding-bottom: 20rpx;
}

.explore-title {
  display: inline-block;
  font-size: 48rpx;
  margin-top: 20rpx;
  color: #9b99a9;
}

.search {
  width: 670rpx;
  height: 64rpx;
  margin-top: 24rpx;
  border: 1rpx solid rgb(164, 169, 179);
  border-radius: 32rpx;
  text-align: center;
}

.explore-types {
	margin-top: 7px;
	width: 670rpx;
}


.explore-types text {
	display: inline-block;
	box-sizing: border-box;
	width: 100%;
	height: 200rpx;
	padding-left: 49rpx;
	background-color: #fff;
	border-radius: 8rpx;
	font-size: 56rpx;
	line-height: 200rpx;
}